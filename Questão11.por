programa {
  funcao inicio() {
  cadeia usuario, senha
   escreva("Digite o seu nome de usuário do Instagram: ")
   leia(usuario)
   escreva("Digite a sua senha de usuário do Instagram: ")
   leia(senha)
   se(usuario=="instaUser" e senha=="senha123"){
    escreva("Login bem-sucedido no Intagram")
   }senao{
    escreva("Usuário ou senha incorretos")
   }
   }
}
