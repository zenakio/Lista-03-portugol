programa {
  funcao inicio() {
  inteiro numero
  escreva("Informe um número: ")
  leia(numero)
  se(numero%3==0){
  escreva("Este número é multiplo de 3.")
  }
  se(numero%5==0){
  escreva("Este número é multiplo de 5.\n")
  }se(numero%3!=0 e numero%5!=0)
  {
  escreva("Este número não é multiplo de 3 nem de 5.")
  }
  }
}
